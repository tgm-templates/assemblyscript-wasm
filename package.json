{
  "name": "@moduleName@",
  "version": "0.1.0",
  "description": "AssemblyScript WebAssembly demo",
  "main": "index.js",
  "keywords": [
    "assemblyscript",
    "wasm"
  ],
  "author": {
    "name": "@github_user_name@"
  },
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized",
    "asbuild:untouched": "asc ./node_modules/as-bind/lib/assembly/as-bind.ts assembly/index.ts -b build/untouched.wasm -t build/untouched.wat --sourceMap --debug",
    "asbuild:optimized": "asc ./node_modules/as-bind/lib/assembly/as-bind.ts assembly/index.ts -b build/optimized.wasm -t build/optimized.wat --sourceMap --optimize",
    "asbuild:watch": "onchange -i 'assembly/**/*' -- npm run asbuild",
    "deno:build": "npm run asbuild && deno run -A --unstable --no-check build.ts"
  },
  "dependencies": {
    "@assemblyscript/loader": "^0.17.0",
    "as-bind": "^0.4.2"
  },
  "devDependencies": {
    "assemblyscript": "^0.17.0",
    "jest": "^26.6.1"
  }
}
